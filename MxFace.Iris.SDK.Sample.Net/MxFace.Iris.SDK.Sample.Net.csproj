<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
	  <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.10" />
	  <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="8.0.10">
		  <PrivateAssets>all</PrivateAssets>
		  <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
	  </PackageReference>
	  <PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="8.0.10" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="MxFace.Iris.SDK.Core">
      <HintPath>bin\Debug\net8.0\MxFace.Iris.SDK.Core.dll</HintPath>
    </Reference>
    <Reference Include="MxFace.Iris.SDK.Data">
      <HintPath>bin\Debug\net8.0\MxFace.Iris.SDK.Data.dll</HintPath>
    </Reference>
    <Reference Include="MxFace.Iris.SDK.Extentions">
      <HintPath>bin\Debug\net8.0\MxFace.Iris.SDK.Extentions.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
